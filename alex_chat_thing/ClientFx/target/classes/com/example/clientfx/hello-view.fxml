<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ap_main" prefHeight="396" prefWidth="478" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clientfx.Controller">
    <children>
        <Button fx:id="button_send" layoutX="404" layoutY="338" onMouseClicked="#on_Button_pressed" text="send" />
        <TextField fx:id="tf_message" layoutX="30" layoutY="338"  onAction="#on_Enter_Pressed"  prefHeight="26" prefWidth="361" />
        <ScrollPane fx:id="sp_main" fitToWidth="true" layoutX="30" layoutY="70" prefHeight="256" prefWidth="418">
            <content>
                <VBox fx:id="vbox_messages" prefHeight="248" prefWidth="412" />
            </content>
        </ScrollPane>
        <Label layoutX="114" layoutY="26" text="WeebChat">
            <font>
                <Font name="Arial Rounded MT Bold" size="23" />
            </font>
        </Label>
    </children>

</AnchorPane>
